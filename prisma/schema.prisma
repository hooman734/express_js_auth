// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Person {
  id String @id
  user_name String @unique
  name String?
  surname String?
  location String?
  birthdate DateTime?
  password String
}